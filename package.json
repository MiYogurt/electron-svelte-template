        "@types/fs-extra": "^5.0.4",
        "@types/iconv-lite": "0.0.1",
        "@types/ora": "^1.3.4",
        "@types/url-join": "^0.8.2",
        "electron": "^2.0.4",
        "electron-builder": "^20.11.1",
        "electron-webpack": "^2.0.1",
        "electron-webpack-ts": "^2.0.2",
        "svelte": "^2.9.1",
        "svelte-loader": "^2.9.1",
        "typescript": "^2.9.2",
        "webpack": "^4.8.1"
    },
    "resolutions": {
        "webpack-sources": "1.0.1"
    }
