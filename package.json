        }
    },
    "dependencies": {
        "baidu-aip-sdk": "^2.3.3",
        "camelcase": "^5.0.0",
        "cheerio": "^1.0.0-rc.2",
        "electron-auto-setting": "^2.1.1",
        "electron-util": "^0.9.0",
        "fast-glob": "^2.2.2",
        "from2": "^2.3.0",
        "fs-extra": "^6.0.1",
        "iconv-lite": "^0.4.23",
        "marked": "^0.4.0",
        "mp3-concat": "^0.1.2",
        "ora": "^2.1.0",
        "phin": "^2.9.0",
        "require-fool-webpack": "^2.0.0",
        "rxjs": "^6.2.1",
        "source-map-support": "^0.5.5",
        "svelte-extras": "^2.0.2",
        "svelte-transitions": "^1.2.0",
